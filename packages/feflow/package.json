{
	"name": "@dxdns/feflow",
	"version": "0.5.38",
	"description": "feflow: svelte components ui",
	"private": false,
	"author": "Di√≥genes Rodrigues <https://github.com/dxdns>",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"homepage": "https://feflow.dxdns.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dxdns/feflow.git"
	},
	"scripts": {
		"build": "npm run prepack",
		"prepack": "svelte-package && publint",
		"postinstall": "node ./postinstall.js"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"postinstall.js"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.0.0",
		"publint": "^0.3.2",
		"svelte": "^5.0.0"
	},
	"keywords": [
		"svelte",
		"svelte5",
		"svelte-ui",
		"ui",
		"feflow",
		"components",
		"ui-components",
		"svelte-pkg",
		"svelte-actions",
		"actions",
		"dxdns"
	]
}
